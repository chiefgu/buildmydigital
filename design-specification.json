  "implementationGuide": {
    "step1_fonts": {
      "instruction": "Import Google Fonts",
      "code": "@import url('https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Space+Grotesk:wght@400;500;600;700&display=swap');"
    },
    "step2_cssVariables": {
      "instruction": "Define CSS custom properties for the design system",
      "code": ":root {\n  /* Colors */\n  --color-primary: #2d2c2b;\n  --color-bg-peach: #f7d2ca;\n  --color-bg-gray: #e4e4e4;\n  --color-bg-dark: #1e1d1d;\n  --color-accent-yellow: #ffea23;\n  --color-accent-green: #48a133;\n  --color-bg-light: #f7f7f7;\n  --color-text-primary: #2d2c2b;\n  --color-text-secondary: #727272;\n  --color-text-light: #828282;\n  --color-white: #ffffff;\n  --color-black: #000000;\n  \n  /* Typography */\n  --font-heading: 'Space Grotesk', sans-serif;\n  --font-body: 'Inter', sans-serif;\n  \n  --font-size-hero: 130px;\n  --font-size-h2: 55px;\n  --font-size-h3: 38px;\n  --font-size-h4: 30px;\n  --font-size-body: 17px;\n  --font-size-small: 14px;\n  \n  --line-height-hero: 112px;\n  --line-height-h2: 55px;\n  --line-height-h3: 40px;\n  --line-height-h4: 44.8px;\n  --line-height-body: 30px;\n  \n  --letter-spacing-hero: -8px;\n  --letter-spacing-h2: -3px;\n  --letter-spacing-h3: -2px;\n  --letter-spacing-button: 0.5px;\n  \n  /* Spacing */\n  --section-padding: 110px 0;\n  --container-max-width: 1220px;\n  --container-padding: 0 15px;\n  \n  /* Effects */\n  --transition: all 0.3s ease-in-out;\n  --shadow-light: 0 5px 20px rgba(0, 0, 0, 0.1);\n  --shadow-medium: 0 0 20px rgba(0, 0, 0, 0.08);\n}"
    },
    "step3_baseStyles": {
      "instruction": "Set up base/reset styles",
      "code": "* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: var(--font-body);\n  font-size: var(--font-size-body);\n  line-height: var(--line-height-body);\n  color: var(--color-text-secondary);\n  background-color: var(--color-white);\n}\n\n.container {\n  max-width: var(--container-max-width);\n  margin: 0 auto;\n  padding: var(--container-padding);\n}\n\nsection {\n  padding: var(--section-padding);\n}"
    },
    "step4_typography": {
      "instruction": "Style headings according to hierarchy",
      "code": "h1 {\n  font-family: var(--font-heading);\n  font-size: var(--font-size-hero);\n  font-weight: 600;\n  line-height: var(--line-height-hero);\n  letter-spacing: var(--letter-spacing-hero);\n  color: var(--color-text-primary);\n  margin-bottom: 30px;\n}\n\nh2 {\n  font-family: var(--font-heading);\n  font-size: var(--font-size-h2);\n  font-weight: 600;\n  line-height: var(--line-height-h2);\n  letter-spacing: var(--letter-spacing-h2);\n  color: var(--color-text-primary);\n}\n\nh3 {\n  font-family: var(--font-heading);\n  font-size: var(--font-size-h4);\n  font-weight: 500;\n  line-height: var(--line-height-h4);\n  color: var(--color-text-primary);\n}\n\nh4 {\n  font-family: var(--font-heading);\n  font-size: var(--font-size-h3);\n  font-weight: 600;\n  line-height: var(--line-height-h3);\n  letter-spacing: var(--letter-spacing-h3);\n  color: var(--color-text-primary);\n}\n\np {\n  font-size: var(--font-size-body);\n  line-height: var(--line-height-body);\n  color: var(--color-text-secondary);\n}"
    },
    "step5_buttons": {
      "instruction": "Create button component styles",
      "code": ".btn {\n  display: inline-block;\n  font-size: var(--font-size-small);\n  font-weight: 500;\n  padding: 18px 34px;\n  text-decoration: none;\n  border: none;\n  cursor: pointer;\n  transition: var(--transition);\n  letter-spacing: var(--letter-spacing-button);\n}\n\n.btn-primary {\n  background-color: var(--color-primary);\n  color: var(--color-white);\n  border: 2px solid var(--color-primary);\n  text-transform: uppercase;\n}\n\n.btn-primary:hover {\n  transform: translateY(-2px);\n}\n\n.btn-secondary {\n  background-color: transparent;\n  color: var(--color-text-secondary);\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  padding: 9px 24px;\n}\n\n.btn-ghost {\n  background: transparent;\n  border: none;\n  padding: 0;\n  font-size: 18px;\n}\n\n.btn-pill {\n  background-color: var(--color-primary);\n  color: var(--color-white);\n  border-radius: 50px;\n  padding: 15px 36px;\n}"
    },
    "step6_sections": {
      "instruction": "Create section background variations",
      "code": ".section-light {\n  background-color: var(--color-bg-light);\n}\n\n.section-dark {\n  background-color: var(--color-primary);\n  color: var(--color-white);\n}\n\n.section-dark h2,\n.section-dark h3 {\n  color: var(--color-white);\n}\n\n.section-peach {\n  background-color: var(--color-bg-peach);\n}\n\n.section-yellow {\n  background-color: var(--color-accent-yellow);\n}\n\n.section-gray {\n  background-color: var(--color-bg-gray);\n}"
    },
    "step7_responsive": {
      "instruction": "Add responsive breakpoints",
      "code": "/* Tablet */\n@media (max-width: 992px) {\n  h1 {\n    font-size: 80px;\n    line-height: 70px;\n    letter-spacing: -4px;\n  }\n  \n  h2 {\n    font-size: 42px;\n    line-height: 42px;\n  }\n  \n  section {\n    padding: 70px 0;\n  }\n}\n\n/* Mobile */\n@media (max-width: 768px) {\n  h1 {\n    font-size: 50px;\n    line-height: 48px;\n    letter-spacing: -2px;\n  }\n  \n  h2 {\n    font-size: 32px;\n    line-height: 34px;\n  }\n  \n  .btn {\n    padding: 14px 28px;\n  }\n  \n  section {\n    padding: 50px 0;\n  }\n}"
    }
  },
